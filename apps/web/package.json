{
  "name": "web",
  "license": "MIT",
  "author": "https://github.com/what-was",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf ./node_modules/.cache ./server/dist ./build ./public/build \"./app/styles/**/*.css\"",
    "build": "run-s build:*",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development binode -- @remix-run/dev:remix dev",
    "docs:serve": "npx prisma-docs-generator serve",
    "setup": "npm install && docker compose up -d && prisma migrate reset --force && npm run validate && npm run test:e2e:run",
    "setup:db": "prisma generate && prisma migrate deploy",
    "setup:seed": "prisma db seed",
    "start": "remix-serve build",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "test": "run-p lint typecheck clear test:e2e:run",
    "clear": "rimraf playwright-report",
    "test:e2e:dev": "playwright test",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "typecheck": "tsc -b",
    "validate": "run-p lint typecheck test:e2e:run"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "jsxSingleQuote": false,
    "plugins": [],
    "printWidth": 80,
    "proseWrap": "always",
    "semi": true,
    "singleAttributePerLine": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@chakra-ui/react": "^2.4.9",
    "@clerk/clerk-sdk-node": "^4.6.0",
    "@clerk/remix": "^2.0.1",
    "@clerk/themes": "^1.2.37",
    "@clerk/types": "^3.24.0",
    "@conform-to/react": "^0.5.0",
    "@conform-to/zod": "^0.5.0",
    "@emotion/css": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@prisma/client": "^4.8.1",
    "@remix-run/node": "^1.10.1",
    "@remix-run/react": "^1.10.1",
    "@remix-run/serve": "^1.10.1",
    "@saas-ui/react": "^1.8.0",
    "@saas-ui/sidebar": "^0.8.0",
    "@saas-ui/theme": "^1.7.0",
    "@saas-ui/theme-glass": "^0.3.0",
    "@tabler/icons": "^1.119.0",
    "@upstash/redis": "^1.19.1",
    "axios": "^1.2.3",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.2",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.7",
    "framer-motion": "^8.4.6",
    "isbot": "^3.6.5",
    "lodash": "^4.17.21",
    "nordigen-node": "^1.2.1",
    "prismjs": "^1.29.0",
    "react": "18",
    "react-dom": "18",
    "react-icons": "^4.7.1",
    "react-table": "^7.8.0",
    "react-window": "^1.8.8",
    "remix-seo": "^0.1.0",
    "remix-utils": "^6.0.0",
    "slate": "^0.88.1",
    "slate-history": "^0.86.0",
    "slate-react": "^0.88.2",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.20.2",
    "zodix": "^0.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.2",
    "@remix-run/dev": "^1.10.1",
    "@remix-run/eslint-config": "^1.10.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^4.10.0",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.18",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-table": "^7.7.14",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/parser": "^5.48.2",
    "autoprefixer": "^10.4.13",
    "binode": "^1.0.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-monorepo": "^0.3.2",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prisma": "^4.9.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=16"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
