    "@types/compression": "^1.7.2",
    "@types/crawler": "^1.2.2",
    "@types/eslint": "^8.4.6",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.13",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "@vitest/coverage-c8": "^0.22.1",
    "autoprefixer": "^10.4.8",
    "c8": "^7.12.0",
    "cookie": "^0.5.0",
